bs_package_flags = -bs-package-name buckle_chess
src_root_dir = /home/caden/OCamlProjects/BuckleChess
bsc = /home/caden/OCamlProjects/BuckleChess/node_modules/bs-platform/lib/bsc.exe
bsdep = /home/caden/OCamlProjects/BuckleChess/node_modules/bs-platform/lib/bsb_helper.exe
warnings = -w -30-40+6+7+27+32..39+44+45+101
bsc_flags = -bs-suffix -nostdlib -I '/home/caden/OCamlProjects/BuckleChess/node_modules/bs-platform/lib/ocaml' -bs-super-errors -w -23 -color always
ppx_flags = 
bs_package_includes = -I /home/caden/OCamlProjects/BuckleChess/node_modules/bucklescript-tea/lib/ocaml -I /home/caden/OCamlProjects/BuckleChess/node_modules/ochess/lib/ocaml
bs_package_dev_includes = 
namespace = 
bsb_dir_group = 0
bsc_lib_includes = -I src
rule build_ast_and_module_sets
  command = ${bsc}  ${pp_flags} ${ppx_flags} ${warnings} ${bsc_flags} -c -o ${out} -bs-syntax-only -bs-binary-ast ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/Main.mlast : build_ast_and_module_sets $src_root_dir/src/Main.ml
rule build_deps
  command = ${bsdep} ${namespace} -g ${bsb_dir_group} -MD ${in}
  description = [34mBuilding[39m [2m${out}[22m
build src/Main.mlast.d : build_deps src/Main.mlast
rule build_cmj_cmi
  command = ${bsc} ${bs_package_flags} -bs-assume-no-mli -bs-no-builtin-ppx-ml -bs-no-implicit-include ${bs_package_includes} ${bsc_lib_includes} ${bsc_extra_includes} ${warnings} ${bsc_flags} -o ${out} -c  ${in} $postbuild
  depfile = ${in}.d
  description = [34mBuilding[39m [2m${out}[22m
build src/Main.cmj |  $src_root_dir/src/Main.bs.js src/Main.cmi : build_cmj_cmi src/Main.mlast
  bs_package_flags = $bs_package_flags  -bs-package-output es6:src
build src/Board.mlast : build_ast_and_module_sets $src_root_dir/src/Board.ml
build src/Board.mlast.d : build_deps src/Board.mlast
build src/Board.cmj |  $src_root_dir/src/Board.bs.js src/Board.cmi : build_cmj_cmi src/Board.mlast
  bs_package_flags = $bs_package_flags  -bs-package-output es6:src
build src/Chess.mlast : build_ast_and_module_sets $src_root_dir/src/Chess.ml
build src/Chess.mlast.d : build_deps src/Chess.mlast
build src/Chess.cmj |  $src_root_dir/src/Chess.bs.js src/Chess.cmi : build_cmj_cmi src/Chess.mlast
  bs_package_flags = $bs_package_flags  -bs-package-output es6:src
build src/ochess.mlast : build_ast_and_module_sets $src_root_dir/src/ochess.ml
build src/ochess.mlast.d : build_deps src/ochess.mlast
build src/ochess.cmj |  $src_root_dir/src/ochess.bs.js src/ochess.cmi : build_cmj_cmi src/ochess.mlast
  bs_package_flags = $bs_package_flags  -bs-package-output es6:src
build build.ninja : phony  ||  src/ochess.mlast.d src/Chess.mlast.d src/Board.mlast.d src/Main.mlast.d
